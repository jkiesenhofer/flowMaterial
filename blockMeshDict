    (185 0 1)       //14
    (295 0 1)       //15
    ($Ly 0 1)       //16
    (0 $N 1)       //17
    ($A $N 1)       //18
    ($A2 $N 1)       //19
    ($Ly $N 1)       //20
    (0 40 1)       //21
    ($A 40 1)       //22
    ($A2 40 1)       //23
    ($Ly 40 1)       //24
);

blocks
(
    hex (0 1 5 4 12 13 17 16) (23 8 1) simpleGrading (1 1 1)
    hex (2 3 7 6 14 15 19 18) (19 8 1) simpleGrading (1 1 1)
